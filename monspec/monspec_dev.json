{
  "spec_version": "1.0",
  "indicators": [
    {
      "id": "heap_committed",
      "source": "Prometheus",
      "query": "heap_committed{instance='carts.dev.svc.cluster.local:80',job='carts'}",
      "grading": {
        "type": "Threshold",
        "thresholds": {
          "upperSevere": 8000000
        },
        "metricScore": 50
      }
    },
    {
      "id": "P90_ResponseTime_Backend",
      "source": "Dynatrace",
      "query": {
        "timeseriesId": "com.dynatrace.builtin:service.responsetime",
        "aggregation": "p90",
        "tags": [
          "backend"
        ]
      },
      "grading": {
        "type": "Threshold",
        "thresholds": {
          "upperSevere": 1000000,
          "upperWarning": 800000,
          "lowerWarning ": 200000,
          "lowerSevere": 100000
        },
        "metricScore": 50
      }
    },
  ],
  "objectives": {
    "pass": 90,
    "warning": 75
  }
}
