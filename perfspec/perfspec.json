{
  "spec_version": "1.0",
  "indicators": [
    {
      "id":"request_latency_seconds",
      "source":"Prometheus",
      "query":"rate(requests_latency_seconds_sum{job='carts-ENV_PLACEHOLDER'}[3m])/rate(requests_latency_seconds_count{job='carts-ENV_PLACEHOLDER'}[3m])",
      "grading":{
          "type":"Threshold",
          "thresholds":{
            "upperSevere":0.8
          },
          "metricScore":50
      }
    },
    {
      "id": "ResponseTime_Backend",
      "source": "Dynatrace",
      "query": {
        "timeseriesId": "com.dynatrace.builtin:service.responsetime",
        "aggregation": "AVG",
        "startTimestamp": "",
        "endTimestamp": ""
      },
      "grading": {
        "type": "Threshold",
        "thresholds": {
          "upperSevere": 1000000,
          "upperWarning": 800000
        },
        "metricScore": 50
      }
    }
  ],
  "objectives": {
    "pass": 90,
    "warning": 75
  }
}
